{
  "name": "{{ kebabCase pluginName }}",
  "version": "1.0.0",
  "description": "{{ sentenceCase pluginDescription }}",
  "scripts": {
    "build": "webpack -c ./webpack.config.ts --env production",
    "dev": "webpack -w -c ./webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "{{ packageManagerName }} run lint{{#if isNPM}} --{{/if}} --fix",
    "e2e": "npx playwright install && npx playwright test smoke/",
    "e2e:codegen": "npx playwright install && playwright codegen",
    "e2e:ci": "npx playwright install && CI=true npx playwright test smoke/",
    "e2e:server": "docker-compose up -d e2e",
    "e2e:server:down": "docker-compose --profile e2e down",
    "server": "docker-compose up -d --build",
    "sign": "npx --yes @grafana/sign-plugin@latest",
    "prepare": "husky install"
  },
  "author": "{{ sentenceCase orgName }}",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/tsconfig": "^1.2.0-rc1",
    "@playwright/test": "^1.36.2",
    "@swc/core": "^1.3.90",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.26",
    "@tanstack/react-query-devtools": "^4.32.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.8.7",{{#if isAppType}}
    "@types/react-router-dom": "^5.3.3",{{/if}}
    "@types/sinon": "^10.0.16",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.2.7",
    "husky": "^8.0.3",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.3",
    "msw": "^1.2.3",
    "playwright-msw": "^2.2.1",
    "prettier": "^2.8.7",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "sinon": "^15.2.0",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsc-files": "^1.1.4",
    "typescript": "4.8.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@emotion/css": "^11.1.3",
    "@grafana/data": "{{ grafanaVersion }}",
    "@grafana/runtime": "{{ grafanaVersion }}",
    "@grafana/ui": "{{ grafanaVersion }}",{{#if_eq pluginType "scenesapp"}}
    "@grafana/schema": "{{ grafanaVersion }}",
    "@grafana/scenes": "0.27.0",{{/if_eq}}
    "@tanstack/react-query": "^4.32.0",
    "date-fns": "^2.30.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",{{#if isAppType}}
    "react-router-dom": "^5.2.0",
    "rxjs": "7.8.0",{{/if}}
    "tslib": "2.5.3",
    "uuid": "^9.0.0"
  },
  "packageManager": "{{ packageManagerName }}@{{ packageManagerVersion }}"
}
